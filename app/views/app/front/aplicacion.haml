        function calcular1()
        {
           var fechaActual = new Date();
           var dia = document.getElementById("dia").value;
           var mes = document.getElementById("mes").value;
           var ano = document.getElementById("ano").value;
        
           var fechaVencimiento = new Date(2024,01,04);

           mes = mes-1;

           var fechaValor = new Date(ano,mes,dia);

           var comision = 0.50;

           var fechaEmision = new Date(2017,06,13);

           var diasCaidos = Math.floor((fechaValor - fechaEmision) / (1000*60*60*24));

           var cupon = 16.25;

           var valorNominal = parseFloat(document.getElementById("nominal").value);

           var precio = parseFloat(document.getElementById("precio").value);

           var valorNeto = Math.abs((valorNominal * precio) / 100);

           var valorNetDecimal = valorNeto.toFixed(2);

           var interesesCaidos = Math.abs((valorNominal * cupon * diasCaidos) / 360 / 100);

           var intcaidosDecimal = interesesCaidos.toFixed(2);

           var valorEfectivo = Math.abs(valorNeto + interesesCaidos);

           var valorEfecDecimal = valorEfectivo.toFixed(2);

           var comisionFinal = Math.abs((valorEfectivo * comision) / 100);

           var comiFinDecimal = comisionFinal.toFixed(2);

           var total = Math.abs(valorEfectivo + comisionFinal);

           var totalDecimal = total.toFixed(2);

           var cuponFinal = Math.abs((valorNominal * cupon) / 100);
          
           var diasVenc = Math.floor((fechaVencimiento - fechaValor) / (1000*60*60*24)); 

           var diasVencFinal = Math.abs(diasVenc / 360);                  

           var rendtoAlto = cuponFinal + (valorNominal - valorNeto) / diasVencFinal;

           var rendtoBajo = Math.abs((valorNominal + valorNeto) / 2); 

           var rendto = Math.abs((rendtoAlto / rendtoBajo) * 100);

           rendto = rendto.toFixed(2);           
            
           rendto = parseFloat( rendto ).toLocaleString();     

           totalDecimal = parseFloat( totalDecimal ).toLocaleString();         

           document.getElementById('resultado').innerHTML= "Rendimiento Efectivo: " + rendto + '%';  
           document.getElementById('resultado2').innerHTML="Total a Pagar en Bs: " + totalDecimal; 
                    
        }

